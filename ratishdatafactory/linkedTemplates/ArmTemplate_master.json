{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ratishdatafactory"
		},
		"LS_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorage'"
		},
		"LS_AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSynapseAnalytics'"
		},
		"LS_PostgreSql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_PostgreSql'"
		},
		"LS_PostgreSql2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_PostgreSql2'"
		},
		"LS_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDB'"
		},
		"LS_adlsG2_jan_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adlsG2_jan'"
		},
		"LS_adlsg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adlsg2'"
		},
		"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://myratishkeyvault.vault.azure.net/"
		},
		"LS_adlsG2_jan_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myratishadlsg2.dfs.core.windows.net"
		},
		"LS_adlsg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myratishadlsg2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ratishdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureBlobStorage_connectionString": {
						"value": "[parameters('LS_AzureBlobStorage_connectionString')]"
					},
					"LS_AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('LS_AzureSynapseAnalytics_connectionString')]"
					},
					"LS_SQLDB_connectionString": {
						"value": "[parameters('LS_SQLDB_connectionString')]"
					},
					"LS_adlsG2_jan_accountKey": {
						"value": "[parameters('LS_adlsG2_jan_accountKey')]"
					},
					"LS_adlsg2_accountKey": {
						"value": "[parameters('LS_adlsg2_accountKey')]"
					},
					"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_adlsG2_jan_properties_typeProperties_url": {
						"value": "[parameters('LS_adlsG2_jan_properties_typeProperties_url')]"
					},
					"LS_adlsg2_properties_typeProperties_url": {
						"value": "[parameters('LS_adlsg2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ratishdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_PostgreSql_connectionString": {
						"value": "[parameters('LS_PostgreSql_connectionString')]"
					},
					"LS_PostgreSql2_connectionString": {
						"value": "[parameters('LS_PostgreSql2_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ratishdatafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "ratishdatafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ratishdatafactory_ArmTemplate_1')]"
			]
		}
	]
}